{
  "name": "react-ziggeo",
  "version": "4.6.2",
  "description": "Ziggeo's react component for easy react application deployment",
  "main": "build/index.es5.js",
  "module": "build/index.es5.js",
  "browser": "build/index.es5.js",
  "exports": {
    ".": {
      "import": "./build/index.es5.js",
      "require": "./build/index.es5.js",
      "default": "./build/index.es5.js"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && webpack --mode production",
    "build:pack": "npm run build && npm pack",
    "rebuild-pack": "rimraf build && npm run build",
    "rebuild-dev-pack": "rimraf build && npm run build",
    "rebuild-npm-dev-pack": "rimraf build && rimraf react-ziggeo-$npm_package_version.tgz && npm run build && npm pack",
    "rebuild-demo-pack": "rimraf build && rimraf react-ziggeo-$npm_package_version.tgz && npm run build && npm pack",
    "start": "concurrently \"webpack-dev-server --mode development --hot\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ziggeo/react-ziggeo"
  },
  "keywords": [
    "react-component",
    "react",
    "ziggeo"
  ],
  "author": {
    "name": "Ziggeo Inc"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Ziggeo/react-ziggeo"
  },
  "homepage": "https://ziggeo.com",
  "peerDependencies": {
    "ziggeo-client-sdk": "~2.43.3"
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-proposal-export-default-from": "^7.27.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.7",
    "babel-loader": "^9.2.1",
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.5",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2",
    "ziggeo-client-sdk": "~2.43.3"
  },
  "sideEffects": [
    "*.css"
  ]
}
